{
	"name": "serialize-error",
	"version": "11.0.2",
	"description": "Serialize/deserialize an error into a plain object",
	"license": "MIT",
	"repository": "sindresorhus/serialize-error",
	"funding": "https://github.com/sponsors/sindresorhus",
	"author": {
		"name": "Sindre Sorhus",
		"email": "sindresorhus@gmail.com",
		"url": "https://sindresorhus.com"
	},
	"type": "module",
	"exports": {
		"import": "./src/index.js",
		"require": "./dist/index.cjs",
		"types": "./src/index.d.ts"
	},
	"engines": {
		"node": ">=14.16"
	},
	"scripts": {
		"//test": "xo && ava && tsd -t src/index.d.ts -f src/index.test-d.ts",
		"test": "prettier -c src && ava && tsd -t src/index.d.ts -f src/index.test-d.ts",
		"build": "tsup",
		"prepare": "npm run build"
	},
	"files": [
		"src/index.js",
		"src/index.d.ts",
		"src/error-constructors.js",
		"src/error-constructors.d.ts",
		"dist"
	],
	"keywords": [
		"error",
		"serialize",
		"stringify",
		"object",
		"convert",
		"process",
		"send",
		"cause",
		"deserialize"
	],
	"dependencies": {
		"type-fest": "^2.12.2"
	},
	"devDependencies": {
		"ava": "^5.3.1",
		"prettier": "^3.0.3",
		"tsd": "^0.29.0",
		"tsup": "^7.2.0",
		"xo": "^0.56.0"
	}
}
